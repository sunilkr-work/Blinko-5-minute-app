{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ClassName": {
    "DarkStoreManager": "DarkStoreManager"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost\\SQLEXPRESS;Database=Blinko;Trusted_Connection=True;TrustServerCertificate=True;"
  },

  "ReplenishThreshold": {
    "default": 1000
  },
  "applicationUrl": "http://localhost:5000",

  "JwtSettings": {
    "Issuer": "Blinko",
    "SecretKey": "I am best developer",
    "Audience": "BlinkoAppUser",
    "Expire": 3
  },

  "RateLimiter": {
    "RetryTimeInSec": 5,
    "Count": 2
  }

}
